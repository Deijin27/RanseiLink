<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#171717"/>
    <SolidColorBrush x:Key="WindowTransparentBackgroundBrush" Color="#1F1F1F"/>
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#3F3F3F" />
    <!--Color="#ada872"/>-->

    <SolidColorBrush x:Key="AccentBrush" Color="#9d8abf"/>

    <SolidColorBrush x:Key="WindowMouseOverBrush" Color="#1d1d1d"/>
    <SolidColorBrush x:Key="WindowTransparentMouseOverBackgroundBrush" Color="#292929"/>
    <SolidColorBrush x:Key="WindowTransparentSelectedBackgroundBrush" Color="#303030"/>

    <SolidColorBrush x:Key="ForegroundBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="AltForegroundBrush" Color="#A2A2A2"/>
    <SolidColorBrush x:Key="ForegroundDisabledBrush" Color="#999999"/>

    <SolidColorBrush x:Key="ControlBorderBrush" Color="#555555"/>
    <SolidColorBrush x:Key="ControlMouseOverBorderBrush" Color="#777777"/>

    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#333333"/>
    <SolidColorBrush x:Key="ButtonMouseOverBrush" Color="#3f3f3f"/>
    <SolidColorBrush x:Key="ButtonPressedBrush" Color="#464646"/>

    <SolidColorBrush x:Key="InputBackgroundBrush" Color="#0E0E0E"/>
    <SolidColorBrush x:Key="InputDisabledBackgroundBrush" Color="#1e1e1e"/>

</ResourceDictionary>